    "version": "0.2.0",
    "configurations": [
        {
            "name": "rufino local",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "toolArgs": [
                "--dart-define-from-file",
                "secrets/local_config.json",
                
            ],
        },
        {
            "name": "rufino prod",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "toolArgs": [
                "--dart-define-from-file",
                "secrets/prod_config.json",
            ],
        },
        {
            "name": "rufino (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define-from-file",
                "secrets/config.json",
            ],
        }
    ]
